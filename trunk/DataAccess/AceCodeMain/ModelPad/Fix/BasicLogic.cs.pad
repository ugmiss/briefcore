using System;
using System.Collections.Generic;
using System.Text;
using DataAccess;

namespace BusinessLogic
{
    public class BasicLogic
    {
        private SqlExecuter _Executer;

        public SqlExecuter Executer
        {
            get
            {
                if (_Executer == null)
                    _Executer = Configuration.GetExecuter();
                return _Executer;
            }
            set { _Executer = value; }
        }
    }
}
