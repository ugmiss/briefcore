<?xml version="1.0"?>
<configuration>
  <system.web>
    <compilation debug="true" />
  </system.web>
  <system.serviceModel>
    <bindings>
      <netTcpBinding>
        <binding name="netTcpbinding" listenBacklog="3" maxConnections="50" maxReceivedMessageSize="72036757" maxBufferSize="72036757" maxBufferPoolSize="78888524" />
      </netTcpBinding>
    </bindings>
    <!--WCF服务ABC-->
    <services>
      <service name="WCFService.MessageService">
        <endpoint address="mex" binding="mexHttpBinding" name="mexEndPoint" contract="IMetadataExchange" listenUriMode="Explicit">
        </endpoint>
        <endpoint address="net.tcp://localhost:1999/MessageService" binding="netTcpBinding" contract="WCFContract.IMessageService">
        </endpoint>
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:2012/MessageService/" />
          </baseAddresses>
        </host>
      </service>
    </services>
    <behaviors>
      <serviceBehaviors>
        <behavior>
          <serviceMetadata httpGetEnabled="True" />
          <serviceDebug includeExceptionDetailInFaults="False" />
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>
</configuration>